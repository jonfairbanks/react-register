{
  "name": "react-register",
  "version": "1.2.0",
  "homepage": "https://bsord.github.io/react-register/",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "highcharts": "^6.1.1",
    "node-sass": "^4.9.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "4.1.2",
    "react": "^16.2.0",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-dom": "16.3.0",
    "react-google-maps": "9.4.5",
    "react-highcharts": "^16.0.2",
    "react-images": "^0.5.19",
    "react-notification-system": "0.2.17",
    "react-photo-gallery": "^6.0.28",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-skillbars": "^1.5.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-old": "npm-run-all -p watch-css start-js",
    "start": "if [ -f src/variables/SiteData.jsx ]; then npm-run-all -p watch-css start-js; else cp src/variables/SiteData.jsx-sample src/variables/SiteData.jsx && npm-run-all -p watch-css start-js; fi",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "load-sample": "cp src/variables/SiteData.jsx-sample src/variables/SiteData.jsx"

  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0"
  }
}
